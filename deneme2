using UnityEngine; 
using System.Collections;
public class AA : MonoBehaviour { 
    public fload moveSpeed;
void Start() {
moveSpeed= 1f; 
}
void Update() {
 transform.Translate(moveSpeed * Input.GetAxis(“Horizantal”) * Time.deltaTime,0f, moveSpeed * Input.GetAxis(“Vertical” * Time.deltaTime,0f;
}
}

